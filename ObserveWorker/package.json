{
  "name": "observe-worker",
  "version": "0.1.0",
  "private": true,
  "description": "Observe worker is responsible for listening on streaming API from BigchainDB",
  "main": "./src/server.js",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node ./dist/server.js",
    "build": "babel src -d dist",
    "serve": "nodemon --exec babel-node ./src/server.js",
    "format": "prettier --write --trailing-comma none --print-width 80 --tab-width 4 'src/**/*.js' 'src/*.js'"
  },
  "author": "Blockchain capstone team",
  "license": "MIT",
  "dependencies": {
    "axios": "0.19.0",
    "noogger": "0.1.3 ",
    "websocket": "1.0.29"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.4",
    "babel-plugin-module-resolver": "3.2.0",
    "dotenv": "8.0.0",
    "nodemon": "1.19.0",
    "prettier": "1.18.2"
  }
}
